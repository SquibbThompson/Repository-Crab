
import numpy as np
import matplotlib.pyplot as plt
import matplotlib.animation as animation

# Modified function to check for inconsistencies and report line number
def string_to_np_array_with_check(binary_string):
    lines = binary_string.split('\n')
    array_list = []
    
    for line_num, line in enumerate(lines, start=1):
        try:
            int_line = list(map(int, line))
            array_list.append(int_line)
        except ValueError:
            print(f"Non-integer value detected on line {line_num}: {line}")
            return None
    
    return np.array(array_list)

# Initialize the 3D grid based on the 4 binary strings
def initialize_3D_grid(binary_strings):
    return np.array([string_to_np_array_with_check(bs) for bs in binary_strings])



